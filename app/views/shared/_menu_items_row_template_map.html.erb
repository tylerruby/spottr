<script type="text/html" id="menu-item-row-template-map">
  <tr data-id="<%%=id%>" data-place-id="<%%=place_id%>" id="dish-<%%=id%>">
    <td style="border-top:none;">
      <strong class="count" style="font-size:18px;"></span>
    </td>
    <td style="border-top:none;" class="hidden-xs">
      <a href="/places/<%%=place_id%>" class="place">
        <img src="<%%= preview_image_url %>">
      </a>
    </td>
    <td style="border-top:none;">
      <a class="btn btn-default btn-sm glyphicon glyphicon-chevron-up js-upvote <%%=upvoted_class%>" href="/api/places/<%%=place_id%>/menu_items/<%%=id%>/up_vote" style="font-size:14px;border-color:#e8e8e8;background:#fff;">
        <%%=votes_count%>
      </a>
    </td>
    <td>
      <span style="font-size:14px;margin-left:5px;margin-top:5px;">
        <a href="/places/<%%=place_id%>" class="place">
          <%%=name%>
        </a>
      </span>
      <br />
      &nbsp;&nbsp;<%%= place_title %>
    </td>
    <td style="border-top:none;">$<%%= price %></td>
    <td>
      <a href="<%%=direction_link%>" target="_blank" class="btn btn-sm btn-default btn-outline fa fa-car pull-right">
      </a>
    </td>
  </tr>
</script>
